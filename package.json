    "rollup": "rollup -c",
    "lint": "eslint src/*.ts",
    "watch": "npm run build:types-check && rollup -c --watch",
    "postversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomRider/apexcharts-card.git"
