    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.46.0",
    "rollup": "^2.79.1",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "semantic-release": "^21.0.7",
