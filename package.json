    "custom-card-helpers": "^1.9.0",
    "home-assistant-js-websocket": "^8.2.0",
    "lit": "^2.7.6",
    "localforage": "^1.10.0",
    "lz-string": "^1.5.0",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
